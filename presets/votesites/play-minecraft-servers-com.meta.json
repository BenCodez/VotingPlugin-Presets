{
  "schemaVersion": 1,
  "id": "votesite:play-minecraft-servers-com",
  "display": {
    "name": "play-minecraft-servers.com (generic)",
    "description": "Generic play-minecraft-servers.com VoteSite preset."
  },
  "match": {
    "domains": [
      "play-minecraft-servers.com",
      "www.play-minecraft-servers.com"
    ],
    "keywords": [
      "com",
      "minecraft",
      "play",
      "play-minecraft-servers.com",
      "servers"
    ]
  },
  "placeholders": {
    "siteKey": {
      "type": "string",
      "label": "VoteSite key",
      "default": "play-minecraft-servers_com"
    },
    "displayName": {
      "type": "string",
      "label": "VoteSite display name",
      "default": "play-minecraft-servers.com"
    },
    "serviceSite": {
      "type": "string",
      "label": "ServiceSite",
      "default": "play-minecraft-servers.com"
    },
    "voteURL": {
      "type": "string",
      "label": "VoteURL",
      "default": "ADD_VOTE_URL_LATER"
    },
    "voteDelay": {
      "type": "string",
      "label": "VoteDelay (blank = omit)",
      "default": "24h"
    },
    "waitUntilVoteDelay": {
      "type": "string",
      "label": "WaitUntilVoteDelay (true/false, blank = omit)",
      "default": ""
    },
    "voteDelayDaily": {
      "type": "string",
      "label": "VoteDelayDaily (true/false, blank = omit)",
      "default": ""
    },
    "voteDelayDailyHour": {
      "type": "string",
      "label": "VoteDelayDailyHour (0\u201323, blank = omit)",
      "default": ""
    }
  },
  "fragments": [
    {
      "path": "presets/votesites/generic.votesites.yml",
      "mergeInto": "VoteSites"
    }
  ],
  "verified": false
}
