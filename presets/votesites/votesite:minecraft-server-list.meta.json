{
  "schemaVersion": 1,
  "id": "votesite:minecraft-server-list",
  "display": {
    "name": "Minecraft Server List (generic)",
    "description": "Generic Minecraft Server List VoteSite preset."
  },
  "match": {
    "domains": [
      "votesite:minecraft-server-list"
    ],
    "keywords": [
      "mcsl",
      "minecraft server list"
    ]
  },
  "placeholders": {
    "siteKey": {
      "type": "string",
      "label": "VoteSite key",
      "default": "MCSL"
    },
    "displayName": {
      "type": "string",
      "label": "VoteSite display name",
      "default": "Minecraft Server List"
    },
    "serviceSite": {
      "type": "string",
      "label": "ServiceSite",
      "default": "MCSL"
    },
    "voteURL": {
      "type": "string",
      "label": "Vote URL",
      "default": "ADD_VOTE_URL_LATER"
    },
    "voteDelay": {
      "type": "string",
      "label": "VoteDelay (blank = omit)",
      "default": "24h"
    },
    "waitUntilVoteDelay": {
      "type": "string",
      "label": "WaitUntilVoteDelay (true/false, blank = omit)",
      "default": ""
    },
    "voteDelayDaily": {
      "type": "string",
      "label": "VoteDelayDaily (true/false, blank = omit)",
      "default": ""
    },
    "voteDelayDailyHour": {
      "type": "string",
      "label": "VoteDelayDailyHour (0â€“23, blank = omit)",
      "default": ""
    }
  },
  "fragments": [
    {
      "path": "presets/votesites/generic.votesites.yml",
      "mergeInto": "VoteSites"
    }
  ],
  "verified": false
}
