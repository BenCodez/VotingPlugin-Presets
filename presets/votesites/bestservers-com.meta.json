{
  "schemaVersion": 1,
  "id": "votesite:bestservers-com",
  "display": {
    "name": "BestServers.com (generic)",
    "description": "Generic BestServers.com VoteSite preset."
  },
  "match": {
    "domains": [
      "bestservers.com",
      "www.bestservers.com"
    ],
    "keywords": [
      "bestservers",
      "bestservers.com",
      "com"
    ]
  },
  "placeholders": {
    "siteKey": {
      "type": "string",
      "label": "VoteSite key",
      "default": "BestServers_com"
    },
    "displayName": {
      "type": "string",
      "label": "VoteSite display name",
      "default": "BestServers.com"
    },
    "serviceSite": {
      "type": "string",
      "label": "ServiceSite",
      "default": "BestServers.com"
    },
    "voteURL": {
      "type": "string",
      "label": "VoteURL",
      "default": "ADD_VOTE_URL_LATER"
    },
    "voteDelay": {
      "type": "string",
      "label": "VoteDelay (blank = omit)",
      "default": "24h"
    },
    "waitUntilVoteDelay": {
      "type": "string",
      "label": "WaitUntilVoteDelay (true/false, blank = omit)",
      "default": ""
    },
    "voteDelayDaily": {
      "type": "string",
      "label": "VoteDelayDaily (true/false, blank = omit)",
      "default": ""
    },
    "voteDelayDailyHour": {
      "type": "string",
      "label": "VoteDelayDailyHour (0\u201323, blank = omit)",
      "default": ""
    }
  },
  "fragments": [
    {
      "path": "presets/votesites/generic.votesites.yml",
      "mergeInto": "VoteSites"
    }
  ],
  "verified": false
}
