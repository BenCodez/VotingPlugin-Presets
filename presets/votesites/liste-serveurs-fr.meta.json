{
  "schemaVersion": 1,
  "id": "votesite:liste-serveurs-fr",
  "display": {
    "name": "Liste-Serveurs (generic)",
    "description": "Generic Liste-Serveurs VoteSite preset."
  },
  "match": {
    "domains": [
      "liste-serveurs.fr",
      "www.liste-serveurs.fr"
    ],
    "keywords": [
      "liste",
      "liste-serveurs",
      "serveurs"
    ]
  },
  "placeholders": {
    "siteKey": {
      "type": "string",
      "label": "VoteSite key",
      "default": "Liste-Serveurs"
    },
    "displayName": {
      "type": "string",
      "label": "VoteSite display name",
      "default": "Liste-Serveurs"
    },
    "serviceSite": {
      "type": "string",
      "label": "ServiceSite",
      "default": "Liste-Serveurs"
    },
    "voteURL": {
      "type": "string",
      "label": "VoteURL",
      "default": "ADD_VOTE_URL_LATER"
    },
    "voteDelay": {
      "type": "string",
      "label": "VoteDelay (blank = omit)",
      "default": "24h"
    },
    "waitUntilVoteDelay": {
      "type": "string",
      "label": "WaitUntilVoteDelay (true/false, blank = omit)",
      "default": ""
    },
    "voteDelayDaily": {
      "type": "string",
      "label": "VoteDelayDaily (true/false, blank = omit)",
      "default": ""
    },
    "voteDelayDailyHour": {
      "type": "string",
      "label": "VoteDelayDailyHour (0\u201323, blank = omit)",
      "default": ""
    }
  },
  "fragments": [
    {
      "path": "presets/votesites/generic.votesites.yml",
      "mergeInto": "VoteSites"
    }
  ],
  "verified": false
}
